within ComputerCooling.Tutorial.T00_Introduction_to_EBM;

model Conductor "Conductor"
  extends   Modelica.Electrical.Analog.Interfaces.TwoPin; // inherit from MSL
  parameter Conductance G = 1e-3;                         // default value
  Current i; // TwoPin in MSL only defines v and pins are called p and n 
equation
  i = p.i;
  i = G*v;    // add Ohm's law
end Conductor;